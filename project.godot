[application]

config/name="JavaBean"
run/main_scene="res://Main.tscn"
config/icon="res://Assets/Logo/JavaBean.svg"
config/macos_native_icon="res://Assets/Logo/JavaBean.jpg"
config/windows_native_icon="res://JavaBean.ico"

[autoload]

JsonData="*res://Inventory/JSONData.gd"
PlayerInventory="*res://Characters/PlayerInventory.gd"
Firebase="*res://UI/Firebase/Firebase.gd"
Load="*res://World/Load.gd"
PlayerStats="*res://Characters/PlayerStats.tscn"
QuestSystem="*res://Quest/QuestSystem.tscn"
RefreshInv="*res://Characters/Actions/ShopAction/Refresh_Inv.gd"
Save="*res://World/Save.tscn"
QuestManager="*res://Quest/QuestManager.gd"
InventoryManager="*res://Inventory/Inventory_Manager.gd"
GlobalVar="*res://global_var.gd"

[display]

window/size/width=1280
window/size/height=720
window/size/always_on_top=true
window/size/test_width=1280
window/size/test_height=720
window/vsync/use_vsync=false
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg", "res://addons/gut/plugin.cfg" )

[gdnative]

singletons=[ "res://addons/godot-git-plugin/git_api.gdnlib" ]

[global]

canvas=false
viewport=false

